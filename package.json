{
  "name": "vue-buttplug-material-component",
  "version": "0.2.0",
  "description": "Vue.js component for Buttplug Client/Server interaction, with vuetify theming",
  "author": "Kyle Machulis <kyle@machul.is>",
  "main": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build:dev": "webpack --progress --hide-modules --mode=development",
    "build": "webpack --progress --hide-modules --mode=production",
    "tslint": "tslint -c tslint.json 'src/**/*.ts' 'tests/**/*.ts'",
    "tslint:fix": "tslint -c tslint.json --fix semicolon,comma,no-var-keyword,ordered-imports 'src/**/*.ts' 'tests/**/*.ts'",
    "build:main": "tsc -p tsconfig.json",
    "pretest": "yarn build:main"
  },
  "peerDependencies": {
    "buttplug": ">=0.6.1",
    "vue": "^2.x",
    "vuetify": ">=1.0.17"
  },
  "devDependencies": {
    "@types/node": "^9.6.7",
    "buttplug": "^0.6.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "noop-loader": "^1.0.0",
    "null-loader": "^0.1.1",
    "ts-loader": "^4.2.0",
    "ts-node": "^6.0.2",
    "tslint": "^5.9.1",
    "typescript": "^2.8.3",
    "uglify-es": "^3.3.9",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "vue": "^2.5.16",
    "vue-loader": "^15.0.4",
    "vue-property-decorator": "^6.0.0",
    "vue-template-compiler": "^2.5.16",
    "vuetify": "^1.0.17",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  },
  "pre-push": [],
  "dependencies": {
    "vue-cookie": "^1.1.4"
  }
